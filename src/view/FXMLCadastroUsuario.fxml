                  <Font size="14.0" />
               </font>
            </JFXTextField>
            <JFXTextField fx:id="txtSenha" labelFloat="true" layoutX="14.0" layoutY="255.0" maxHeight="35.0" maxWidth="271.0" minHeight="35.0" minWidth="240.0" prefHeight="35.0" prefWidth="270.0" promptText="Senha" styleClass="textField" stylesheets="@css/fxmlcadastrousuario.css">
