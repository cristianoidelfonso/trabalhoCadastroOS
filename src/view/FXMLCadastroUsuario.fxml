            <JFXComboBox fx:id="cbPerfil" labelFloat="true" layoutX="349.0" layoutY="183.0" prefHeight="35.0" prefWidth="190.0" promptText="Perfil" styleClass="comboBox" stylesheets="@css/fxmlcadastrousuario.css" visibleRowCount="12" />
